<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/shareReferenceTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textAppearance="?android:attr/textAppearanceLarge" />
    <View
        android:layout_height="2dip"
       	android:layout_width="fill_parent"
       	android:background="#FF909090" />
    <TextView
        android:id="@+id/codeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CODE:" />
    <TextView
        android:id="@+id/isinTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ISIN:" />
    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name:" />
    <TextView
        android:id="@+id/minRoundLotTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Min round lot:" />
    <TextView
        android:id="@+id/marketPhaseTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Market phase:" />
    <TextView
        android:id="@+id/lastContractPriceTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last contract price:" />
    <TextView
        android:id="@+id/percVarTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Percentual variation:" />
    <TextView
        android:id="@+id/variationTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Variation:" />
    <TextView
        android:id="@+id/lastContractDateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last contract date:" />
    <TextView
        android:id="@+id/buyPriceTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy price:" />
    <TextView
        android:id="@+id/sellPriceTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sell price:" />
    <TextView
        android:id="@+id/lastAmountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last amount:" />
    <TextView
        android:id="@+id/buyAmountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy amount:" />
    <TextView
        android:id="@+id/sellAmountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sell amount:" />
    <TextView
        android:id="@+id/totalAmountTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total amount:" />
    <TextView
        android:id="@+id/maxTodayTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Max today:" />
    <TextView
        android:id="@+id/minTodayTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Min today:" />
    <TextView
        android:id="@+id/maxYearTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Max year:" />
    <TextView
        android:id="@+id/minYearTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Min year:" />
    <TextView
        android:id="@+id/maxYearDateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Max year date:" />
    <TextView
        android:id="@+id/minYearDateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Min year date:" />
    <TextView
        android:id="@+id/lastCloseTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last close:" />
    <TextView
        android:id="@+id/lastUpDateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last update date:" />
</LinearLayout>